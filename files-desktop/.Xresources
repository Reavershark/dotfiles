! Font rendering
Xft.dpi: 180
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! Colors
*foreground: #ffffff
*foreground_bold: #a8a19f
*background: #1b1918

! Black + DarkGrey
*color0: #1b1918
*color8: #686868

! DarkRed + Red
*color1: #ff5c57
*color9: #ff5c57

! DarkGreen + Green
*color2: #5af78e
*color10: #5af78e

! DarkYellow + Yellow
*color3: #f3f99d
*color11: #f3f99d

! DarkBlue + Blue
*color4: #57c7ff
*color12: #57c7ff

! DarkMagenta + Magenta
*color5: #ff6ac1
*color13: #ff6ac1

! DarkCyan + Cyan (both not tango)
*color6: #9aedfe
*color14: #9aedfe

! LightGrey + White
*color7: #eff0eb
*color15: #f1f1f0

! rxvt-unicode
URxvt.intensityStyles: false
URxvt.background: [80]#000000
URxvt.depth: 32
URxvt.font: xft:monospace:size=22
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.saveLines: 100000
URxvt.cursorColor: white
URxvt.perl-ext-common: default,matcher,resize-font,keyboard-select
URxvt.perl-lib: $HOME/.config/Scripts
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.colorUL: #4682B4
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.iso14755: false
URxvt.iso14755_52: false

! rofi
rofi.lines: 18
rofi.modi: drun,run,ssh
rofi.font: Monospace Regular 12
rofi.dpi: 192
rofi.theme: ~/.cache/wal/colors-rofi-dark.rasi
rofi.drun-icon-theme: Paper
rofi.sidebar-mode: true
