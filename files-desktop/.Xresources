! rxvt-unicode
URxvt.intensityStyles: false
URxvt.depth: 32
URxvt.font: xft:DejaVu Sans Mono:size=22
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.saveLines: 100000
URxvt.cursorColor: white
URxvt.perl-ext-common: default,matcher,resize-font,keyboard-select
URxvt.perl-lib: $HOME/.config/Scripts
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.colorUL: #4682B4
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.iso14755: false
URxvt.iso14755_52: false

! rofi
rofi.lines: 18
rofi.modi: drun,run,ssh
rofi.font: Monospace Regular 12
rofi.dpi: 192
rofi.theme: ~/.cache/wal/colors-rofi-dark.rasi
rofi.drun-icon-theme: Paper
rofi.sidebar-mode: true
